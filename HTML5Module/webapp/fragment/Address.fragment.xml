	
<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:Form id="FormChange354"
			editable="false">
			<f:title>
				<core:Title text="Address" />
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout
					labelSpanXL="3"
					labelSpanL="3"
					labelSpanM="3"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="4"
					emptySpanL="4"
					emptySpanM="4"
					emptySpanS="0"
					columnsXL="1"
					columnsL="1"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer>
					<f:formElements>
						<f:FormElement label="CityName">
							<f:fields>
								<Label text="{ModelAddress>/ModelAddress/d/CityName}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Country">
							<f:fields>
								<Label text="{ModelAddress>/ModelAddress/d/Country}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="ZIP Code/City">
							<f:fields>
								<Label text="{ModelAddress>/ModelAddress/d/PostalCode}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="StreetName">
							<f:fields>
								<Label text="{ModelAddress>/ModelAddress/d/StreetName}"/>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>